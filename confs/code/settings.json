{
    "[python]": {
        "editor.formatOnType": true
    },
    "remote.SSH.connectTimeout": 60,
    "remote.SSH.useLocalServer": true,
    "remote.SSH.showLoginTerminal": true,
    "remote.autoForwardPorts": false,
    "window.titleBarStyle": "custom",
    "window.title": "${dirty}${activeEditorShort}",
    "remote.SSH.remotePlatform": {
        "oleina.c.googlers.com": "linux"
    },
    "update.mode": "none",
    "terminal.integrated.defaultProfile.linux": "fish",
    "catppuccin.accentColor": "flamingo",
    "catppuccin.italicComments": false,
    "catppuccin.italicKeywords": false,
    "editor.fontFamily": "FiraCode Nerd Font",
    "editor.fontLigatures": true,
    "editor.inlineSuggest.enabled": true,
    "workbench.editor.navigationScope": "editorGroup",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "variable.parameter",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "constant",
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "explorer.confirmDragAndDrop": false,
    "julia.symbolCacheDownload": true,
    "terminal.integrated.allowChords": false,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "terminal.integrated.fontFamily": "FiraCode Nerd Font",
    "workbench.colorTheme": "Catppuccin Macchiato",
    "workbench.iconTheme": "catppuccin-macchiato",
    "workbench.editor.showTabs": false,
    "workbench.list.automaticKeyboardNavigation": false,
    "editor.minimap.enabled": true,
    "editor.scrollbar.vertical": "hidden",
    "dance.menus": {
        "goto": {
            "items": {
                "m": {
                    "text": "to previous marker",
                    "command": "errorLens.selectProblem"
                }
            }
        },
        "file-explorer": {
            "items": {
                "F": {
                    "command": "copyRelativeFilePath",
                    "text": "Copy relative file path"
                },
                "N": {
                    "command": "explorer.newFolder",
                    "text": "New file"
                },
                "O": {
                    "command": "explorer.openAndPassFocus",
                    "text": "Open"
                },
                "c": {
                    "command": "renameFile",
                    "text": "Rename file"
                },
                "d": {
                    "command": "moveFileToTrash",
                    "text": "Delete file"
                },
                "f": {
                    "command": "copyFilePath",
                    "text": "Copy file path"
                },
                "n": {
                    "command": "explorer.newFile",
                    "text": "New folder"
                },
                "o": {
                    "command": "explorer.openWith",
                    "text": "Open with..."
                },
                "p": {
                    "command": "filesExplorer.paste",
                    "text": "Paste file"
                },
                "r": {
                    "command": "revealFileInOS",
                    "text": "Reveal in file explorer"
                },
                "v": {
                    "command": "filesExplorer.openFilePreserveFocus",
                    "text": "View"
                },
                "x": {
                    "command": "filesExplorer.cut",
                    "text": "Cut file"
                },
                "y": {
                    "command": "filesExplorer.copy",
                    "text": "Yank file"
                }
            }
        }
    },
    "window.menuBarVisibility": "compact",
    "workbench.activityBar.location": "hidden",
    "workbench.statusBar.visible": false,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 5000,
    "catppuccin.workbenchMode": "flat",
    "workbench.startupEditor": "none",
    "dynoFileUtils.confirmDelete": false,
    "editor.minimap.renderCharacters": false,
    "editor.cursorBlinking": "solid"
}
